<body>

    <%- include('./partials/header') %>

    <main class="oba_ok_voltar" >

   
        <div class="registro_login_ok__1">

            <div class="login">
            
            
                <form action="">
                    <h2>Login</h2>
                    <br>
                    <input class="nome" type="text" placeholder="Nome">
                    <br><br>
                    <input class="nome" type="password" placeholder="Senha">
                    <br><br>
                    <button class="btnLogin" type="submit">ENTRAR</button>
                    
                    <div class="ficarLogado">
    
                        <div class="check">
                            <input type="checkbox" class="form-check-input" id="ckeck" name="check">
                            <label class="form-check-label" for="lembrarUsuario">Manter logado</label>
                        </div>
        
                     </div>
                     <P></P>
                    
                    <a class="botao_criar_conta" href="/cadastroUsuario">CRIAR CONTA</a>
                </form>
    
    
            </div>
        
        </div>


              

                


            
            
           

